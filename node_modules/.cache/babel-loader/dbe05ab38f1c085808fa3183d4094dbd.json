plate>                                                                      \n                                  </el-table-column>\n\n                                  <el-table-column\n                                    align=\"center\"\n                                    prop=\"shoukuanma\"\n                                    label=\"支付宝收款码\"\n                                    min-width=\"120\">\n                                  </el-table-column>\n                                  <el-table-column\n                                    align=\"center\"\n                                    prop=\"saleman\"\n                                    label=\"收款码业务员\"\n                                    min-width=\"120\">\n                                     <template slot-scope=\"scope\">\n                                         <el-dropdown v-if=\"!scope.row.saleman\" size=\"mini\"   trigger=\"click\">\n                                           <el-button class=\"sys-Btn\" size=\"mini\">\n                                             选择<i class=\"el-icon-arrow-down el-icon--right\"></i>\n                                           </el-button>\n                                           <el-dropdown-menu slot=\"dropdown\">\n                                             <el-dropdown-item>业务员1</el-dropdown-item>\n                                             <el-dropdown-item>业务员2</el-dropdown-item>\n                                             <el-dropdown-item>业务员3</el-dropdown-item>\n                                           </el-dropdown-menu>\n                                         </el-dropdown>\n                                         <template v-else>\n                                             {{scope.row.saleman}}\n                                         </template>\n                                     </template>\n                                  </el-table-column> \n                                  <el-table-column\n                               