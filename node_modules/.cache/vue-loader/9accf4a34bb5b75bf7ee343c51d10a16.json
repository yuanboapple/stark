             </div>\n                  <router-view></router-view>\n              </el-main>\n            </el-container>\n        </el-container>\n    </div>\n</template>\n<script>\nimport MyNavBar  from '../components/MyNavBar'  \nimport store from '../store'\nexport default {\n    components:{\n    \"my-Nav-bar\": MyNavBar\n    },\n    data() {\n        return {\n        }\n    },\n    methods: {\n        /*\n        **用户退出登录 清理登录信息\n        */\n        async logOut(){\n            let resData = await this.http.get('/logout.api')\n            if(resData.code === 200){\n                this.$message.success(resData.msg)\n                localStorage.clear()\n                this.$router.replace({name:\"mylogin\"})\n            }\n        }\n    },\n    created() {\n    },\n}\n</script>\n<style lang=\"less\" >\n.container{\n    width: 100%;\n    height: 100%;\n}\n#container-Wrap{\n    width: 100%;\n    height: 100%;\n}\n.el-header{\n    padding: 0 20px;\n    color: #ffffff;\n    background-color: #36404A;\n    height: 5%;\n    #headerRow{\n        height: 100%;\n    }\n    #logoTitle{\n        font-size: 1.75rem;\n        margin-right: 1.2vw;\n    }\n    #title{\n        font-size: 1.75rem;\n    }\n    .userWrap{\n        display: flex;\n        align-items: center;\n      #loginUserName{\n          font-size: 1rem; \n          margin-right:1.2vw;\n      }\n      #logoutImg{\n          margin-left:1.2vw;\n          margin-right: 0.4vw;\n      }\n      #logout{\n          font-size: 1.125rem;\n          color: #fff;\n      }\n      .devideline{\n        width: 1px;\n        height: 29px;\n        background-color: #fff;\n    }\n    }\n\n}\n</style>\n"]}]}